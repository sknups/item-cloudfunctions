{
  "name": "item-cloudfunctions",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "start": "tsc -p tsconfig.dev.json && concurrently \"tsc -p tsconfig.dev.json -w\" \"nodemon --watch ./dist --exec NODE_ENV=development functions-framework --target=devRouter\"",
    "test": "jest --config test/jest-config.json",
    "test:mocha": "NODE_ENV=development LOG_FORMAT=simple LOG_LEVEL=error ts-mocha test-mocha/**/*.test.ts",
    "lint": "eslint --ext .js,.ts ."
  },
  "dependencies": {
    "@google-cloud/datastore": "^7.0.0",
    "@google-cloud/functions-framework": "^3.1.2",
    "@google-cloud/pubsub": "^3.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/jest": "^29.2.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "mocha": "^10.2.0",
    "mock-express-response": "^0.3.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
